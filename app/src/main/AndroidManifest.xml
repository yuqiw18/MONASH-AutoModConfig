<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yuqiwang.automobilemodificationconfigurator">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main" />
        <activity android:name=".Member" />
        <activity android:name=".ConfiguratorBrand" />
        <activity android:name=".ConfiguratorModel" />
        <activity android:name=".ConfiguratorBadge" />
        <activity android:name=".Previewer" />
        <activity android:name=".Checkout" />
        <activity android:name=".CheckoutServiceCenter" />
        <activity android:name=".CheckoutPaymentMethod" />
        <activity android:name=".OrderTracking"></activity>
    </application>

</manifest>